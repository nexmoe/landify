{
  "name": "landify",
  "version": "0.3.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/nexmoe/landify.git"
  },
  "keywords": [],
  "author": "Nexmoe",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s gen && unbuild",
    "dev": "run-s gen && run-s docs:dev",
    "gen": "run-p gen:*",
    "gen:index": "vite-node ./scripts/genIndex.ts && prettier src/index.ts --write",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint --cache .",
    "lint:ts": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "prepublish": "run-s build"
  },
  "dependencies": {
    "@fancyapps/ui": "^5.0.28",
    "@xiaoshuapp/theme": "^1.0.4",
    "gsap": "^3.12.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@unocss/eslint-config": "^0.58.0",
    "@unocss/postcss": "^0.58.0",
    "@unocss/preset-wind": "^0.58.0",
    "consola": "^3.2.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.0",
    "mkdist": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.58.0",
    "vite-node": "^0.34.6",
    "vitepress": "1.0.0-rc.31",
    "vue": "^3.3.7"
  }
}
