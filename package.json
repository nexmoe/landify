{
  "name": "landify",
  "version": "0.3.5",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/nexmoe/landify.git"
  },
  "keywords": [],
  "author": "Nexmoe",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s gen && unbuild",
    "dev": "run-s gen && run-s docs:dev",
    "gen": "run-p gen:*",
    "gen:index": "vite-node ./scripts/genIndex.ts && prettier src/index.ts --write",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint --cache .",
    "lint:ts": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublish": "run-s build"
  },
  "dependencies": {
    "@fancyapps/ui": "5.0.35",
    "@xiaoshuapp/theme": "1.0.4",
    "gsap": "3.12.5",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@unocss/eslint-config": "0.58.6",
    "@unocss/postcss": "0.58.6",
    "@unocss/preset-wind": "0.58.6",
    "consola": "3.2.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.23.0",
    "fs-extra": "11.2.0",
    "globby": "14.0.1",
    "mkdist": "1.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "tailwindcss": "3.4.1",
    "typescript": "5.4.3",
    "unbuild": "2.0.0",
    "unocss": "0.58.6",
    "vite-node": "2.1.3",
    "vitepress": "1.0.1",
    "vue": "3.4.21"
  }
}
